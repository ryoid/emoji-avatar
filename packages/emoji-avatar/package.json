{
  "name": "emoji-avatar",
  "version": "1.0.0",
  "description": "Emoji Avatars generator for fun.",
  "keywords": [
    "avatar",
    "gravatar",
    "emoji",
    "avatar generator"
  ],
  "author": "Ryan Conceicao",
  "license": "MIT",
  "repository": "ryoid/emoji-avatar",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "dev": "pkgroll --watch",
    "build": "pkgroll --minify",
    "type-check": "tsc --noEmit",
    "prepack": "pnpm build && clean-pkg-json"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "clean-pkg-json": "^1.2.0",
    "pkgroll": "^2.1.1",
    "typescript": "^5.5.3"
  }
}
